{
  "name": "boilerplatevitejs",
  "version": "1.0.0",
  "description": "Boilerplate for development with ViteJS",
  "author": "Maxime Pietrucci-Blacher",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:vite": "vite build",
    "lint": "bunx eslint \"./sources/**/*.js\"",
    "lint:style": "bunx stylelint \"./sources/src/**/*.css\"",
    "preview": "vite preview",
    "format": "bunx prettier --write \"./**/*.{html,css,js}\"",
    "test": "vitest"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@html-eslint/eslint-plugin": "^0.46.0",
    "@html-eslint/parser": "^0.46.0",
    "@isnotdefined/stylelint-plugin": "^6.0.0",
    "eslint": "9.33.0",
    "eslint-config-async": "^3.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unicorn": "^60.0.0",
    "globals": "^16.2.0",
    "prettier": "3.6.2",
    "stylelint": "16.23.1",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-declaration-strict-value": "^1.10.11",
    "stylelint-group-selectors": "^1.0.10",
    "stylelint-rem-over-px": "^1.0.2",
    "stylelint-selector-no-empty": "^1.0.9",
    "stylelint-selector-tag-no-without-class": "^3.0.1",
    "stylelint-use-nesting": "6.0.0",
    "stylelint-value-no-exposed-custom-properties": "^2.3.1",
    "vite": "7.1.4",
    "vitest": "3.2.4"
  }
}
