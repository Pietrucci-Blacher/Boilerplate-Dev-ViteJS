{
    "env": {
        "browser": false,
        "commonjs": true,
        "es2022": true,
        "node": true,
        "worker": true,
        "mocha": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "globals": {},
    "parserOptions": {
        "ecmaVersion": 2022
    },
    "rules": {
        "indent": ["error", "tab"],
        "linebreak-style": "off",
        "quotes": ["warn", "single"],
        "semi": ["error", "always"],
        "no-console": "off",
        "space-before-blocks": "error"
    }
}
